{
  "name": "speechee",
  "version-semver": "0.2.1",
  "maintainers": [
    "Denys Asauliak <d.asauliak@gmail.com>"
  ],
  "supports": "linux and !windows",
  "dependencies": [
    {
      "name": "jarvisto",
      "version>=": "0.3.2"
    },
    {
      "name": "boost-core",
      "version>=": "1.81.0"
    },
    {
      "name": "boost-json",
      "version>=": "1.81.0"
    },
    "glibmm",
    {
      "name": "google-cloud-cpp",
      "default-features": false,
      "version>=": "2.13.0",
      "features": [
        "texttospeech"
      ]
    },
    "gstreamer",
    "libsigcpp",
    "libconfig"
  ],
  "features": {
    "cli": {
      "description": "CLI supporting",
      "dependencies": [
        {
          "name": "boost-program-options",
          "version>=": "1.81.0"
        },
        {
          "name": "libsndfile",
          "version>=": "1.2.0",
          "default-features": false
        }
      ]
    },
    "http": {
      "description": "HTTP API supporting",
      "dependencies": [
        {
          "name": "boost-asio",
          "version>=": "1.81.0",
          "features": [
            "ssl"
          ]
        },
        {
          "name": "boost-beast",
          "version>=": "1.81.0"
        }
      ]
    },
    "tests": {
      "description": "Tests supporting",
      "dependencies": [
        "gtest",
        {
          "name": "libsndfile",
          "version>=": "1.2.0",
          "default-features": false
        }
      ]
    }
  }
}
