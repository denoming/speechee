{
  "name": "speechee",
  "version-semver": "0.2.1",
  "maintainers": [
    "Denys Asauliak <denoming@gmail.com>"
  ],
  "supports": "linux and !windows",
  "dependencies": [
    "jarvisto",
    {
      "name": "boost-core",
      "version>=": "1.81.0"
    },
    {
      "name": "boost-json",
      "version>=": "1.81.0"
    },
    "glibmm",
    {
      "name": "google-cloud-cpp",
      "default-features": false,
      "version>=": "2.13.0",
      "features": [
        "texttospeech"
      ]
    },
    {
      "name": "gstreamer",
      "default-features": false,
      "features": [
        "plugins-base",
        "plugins-good"
      ]
    },
    "libsigcpp",
    {
      "name": "libconfig",
      "version>=": "1.7.3#1"
    }
  ],
  "features": {
    "cli": {
      "description": "CLI supporting",
      "dependencies": [
        "jarvisto",
        {
          "name": "boost-core",
          "version>=": "1.81.0"
        },
        {
          "name": "boost-program-options",
          "version>=": "1.81.0"
        },
        {
          "name": "boost-json",
          "version>=": "1.81.0"
        }
      ]
    },
    "http": {
      "description": "HTTP API supporting",
      "dependencies": [
        {
          "name": "boost-asio",
          "version>=": "1.81.0",
          "features": [
            "ssl"
          ]
        },
        {
          "name": "boost-beast",
          "version>=": "1.81.0"
        }
      ]
    },
    "tests": {
      "description": "Tests supporting",
      "dependencies": [
        "gtest"
      ]
    }
  }
}
